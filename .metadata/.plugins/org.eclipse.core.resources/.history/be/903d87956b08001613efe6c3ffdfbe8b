package org.unpas.ProjectJunit;

import java.lang.reflect.Array;

public class Orang {
	private String nama;
	private int umur;
	Private String isiTas[] = new String[3];
	
	public String getNama() {
		return nama;
	}
	
	public void setNama(String nama) {
		this.nama = nama;
	}
	
	public int getUmur() {
		return umur;
	}
	
	public void setUmur(int umur) {
		this.umur = umur;
	}
	
	public Array[] getIsiTas() {
		return isiTas;
	}
	
	public void setIsiTas(Array[] isiTas) {
		this.isiTas = isiTas;
	}
}
